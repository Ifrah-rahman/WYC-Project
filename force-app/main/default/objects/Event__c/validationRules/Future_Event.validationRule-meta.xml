<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Future_Event</fullName>
    <active>true</active>
    <errorConditionFormula>Event_Date__c &lt;= TODAY()</errorConditionFormula>
    <errorDisplayField>Event_Date__c</errorDisplayField>
    <errorMessage>Event Date is in the future.</errorMessage>
</ValidationRule>
